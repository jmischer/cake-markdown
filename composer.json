{
    "name": "jmischer/cake-markdown",
    "description": "JMischer/CakeMarkdown plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "^3.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "JMischer\\CakeMarkdown\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JMischer\\CakeMarkdown\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
